<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.businessmanagement.PrimaryController">

    <left>
        <VBox fx:id="panelOrga" alignment="TOP_CENTER" prefWidth="160.0" spacing="12.0" style="-fx-background-color: #f0f0f0; -fx-padding: 16; -fx-border-color: #dcdcdc; -fx-border-width: 0 1 0 0;">
            <children>
                <Button fx:id="botonNuevo" mnemonicParsing="false" prefWidth="120.0" style="-fx-background-color: #6dd66d; -fx-font-weight: bold; -fx-text-fill: white;" text="Nuevo" translateY="150.0" />
                <Button fx:id="botonAbrir" mnemonicParsing="false" prefWidth="120.0" style="-fx-background-color: #4da6ff; -fx-font-weight: bold; -fx-text-fill: white;" text="Abrir" translateY="200.0" />
            </children>
        </VBox>
    </left>

    <center>
        <AnchorPane fx:id="panelContenido" style="-fx-background-color: #ffffff;">
            <children>
                <ImageView fx:id="centerImage" fitHeight="140.0" fitWidth="170.0" layoutX="315.0" layoutY="120.0" managed="false" pickOnBounds="true" preserveRatio="true" translateX="-150.0" translateY="25.0" visible="false" />
                <Label fx:id="centerLabel" layoutX="340.0" layoutY="270.0" managed="false" style="-fx-font-size: 14px; -fx-font-weight: bold;" text="Nueva empresa" translateX="-158.0" visible="false" />
            </children>
        </AnchorPane>
    </center>
</BorderPane>
